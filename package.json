{
  "name": "fleetmanager-pro",
  "version": "1.0.0",
  "description": "Fleet Management System with Angular Frontend and ASP.NET Core Backend",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:dev": "ng serve --configuration development --port 4200",
    "build:prod": "ng build --configuration production",
    "api:start": "cd FleetManagerPro.API && dotnet run",
    "dev": "concurrently \"npm run api:start\" \"npm run start\"",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/fire": "^17.1.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@microsoft/signalr": "^9.0.6",
    "firebase": "^10.14.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "concurrently": "^8.2.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.0"
  },
  "keywords": [
    "fleet-management",
    "angular",
    "asp.net-core",
    "mysql",
    "firebase",
    "typescript",
    "thesis-project"
  ],
  "author": "Fleet Management Team",
  "license": "MIT"
}
